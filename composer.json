{
  "name": "soltancode/return-response",
  "type": "library",
  "description": "Simple package for returning standard json responses for Laravel Framework.",
  "version": "v1.0.1",
  "keywords": [
    "soltancode",
    "return",
    "response",
    "laravel",
    "json",
    "rest",
    "api",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Soltan Abilgasimzada",
      "email": "soltancode@gmail.com",
      "homepage": "https://soltancode.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^9.0",
    "illuminate/http": "^9.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Soltancode\\ReturnResponse\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Soltancode\\ReturnResponse\\ReturnResponseServiceProvider"
      ],
      "aliases": {
        "ReturnResponse": "Soltancode\\ReturnResponse\\Facades\\ReturnResponse"
      }
    }
  }
}
